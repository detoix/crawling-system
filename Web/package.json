    "amqplib": "^0.5.6",
    "babel-polyfill": "^6.26.0",
    "express": "^4.16.3",
    "randomcolor": "^0.5.4",
    "react": "^16.5.2",
    "react-csv": "^2.0.3",
    "react-dom": "^16.5.2",
    "react-router-dom": "^5.2.0",
    "react-tag-cloud": "^1.3.2",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "socketio-jwt": "^4.6.2"
  },
  "devDependencies": {
